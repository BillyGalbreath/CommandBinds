{
  "schemaVersion": 1,
  "id": "commandbinds",
  "version": "${version}",
  "name": "CommandBinds",
  "description": "Adds ability to add your own command keybinds",
  "authors": [
    "BillyGalbreath"
  ],
  "contact": {
    "homepage": "https://github.com/BillyGalbreath/CommandBinds/",
    "sources": "https://github.com/BillyGalbreath/CommandBinds/",
    "issues": "https://github.com/BillyGalbreath/CommandBinds/issues/"
  },
  "license": "MIT",
  "icon": "assets/commandbinds/textures/logo.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.pl3x.commandbinds.CommandBinds"
    ]
  },
  "mixins": [
    "commandbinds.mixins.json"
  ],
  "accessWidener": "commandbinds.accesswidener",
  "depends": {
    "fabricloader": ">=${fabricLoaderVersion}",
    "fabric": ">=${fabricApiVersion}",
    "minecraft": "~${minecraftVersion}",
    "java": ">=17"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/kHWFx7bFZh",
        "modrinth.download.link": "https://modrinth.com/mod/commandbinds/"
      }
    }
  }
}
